<div class="container">
<%= simple_form_for [@butler,  @review] do |f| %>
  <div class="form-inputs">
    <%= f.input :rating, collection: 0..5 %>
  </div>
  <div class="form-inputs">
    <%= f.input :description %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
</div>
