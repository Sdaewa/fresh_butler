<div class="container">
  <% butler = User.find(@review.butler_id) %>
  <% user = User.find(@review.client_id) %>
  <h1>Review</h1>
  <h2><%= butler.first_name %> <%= butler.last_name %></h2>
  <p><%= @review.rating %></p>
  <p> <%= @review.description %> </p>
  <p><%= @review.created_at %></p>
  <p><%= user.full_name %></p>
</div>
