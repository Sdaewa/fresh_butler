<div class="row">
    <div class="col-sm-12 col-md-6">
      <h3>Reviews</h3>
        <table class="table table-bordered">
          <tbody>
            <thead>
              <th>Butler</th>
            </thead>
            <% @reviews.each do |review| %>
              <tr>
                <td><%= review.butler.full_name %></td>
                <td><%= link_to "View review", user_review_path(review) %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
    </div>
</div>
